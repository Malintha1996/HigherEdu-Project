<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
  <link href="assets/css/sidebar.css" rel="stylesheet" type="text/css">
  <script src="assets/js/sidebar.js" type="text/javascript"></script>
  <link href="assets/css/navpannel.css" rel="stylesheet" type="text/css">
  <link href="assets/css/institute_account.css" rel="stylesheet" type="text/css">
  <script src="assets/js/addcourse.js" type="text/jscript"></script>

</head>
<body>
    <% include partials/navpannel.ejs %>
    <div id="wrapper" style="margin-top:10vh;">
      <!--sidebar-->
      <div id="sidebar-wrapper" style="min-height:100%;" >
        <ul class="sidebar-nav">

             <div  class="row" id="buttondiv" style="margin-bottom:6px;">
              <li><button id="menu-toggle"  type="button"><img src="assets/icons/menu.svg" class="image-responsive" style="width:20;"></span></button></li>
            </div>
            <div class="row"  style="margin-bottom:3vh;">
               <li><img id="profile-image" src="assets/images/sakaya-logo.jpg" class="rounded-circle" alt="Cinque Terre" ></li>
            </div>
            <div  class="row" id="profdiv" style="margin-bottom:10vh;">
            <li ><p id="profile-name" style="font-family:san-serif;font-size:160%;color:white;">Sakya Higher Educational Institute</p></li>
          </div>
            <div name="siderow" class="row">
             <li><a href="/addCourse"><img src="assets/icons/courses.svg" class="image-responsive" style="width:30;float:left;"><p class="listname">Add Course</p></a></li>
           </div>
           <div name="siderow" class="row">
            <li><a href="#"><img src="assets/icons/settings.svg" class="image-responsive" style="width:30;float:left;"><p class="listname" >Settings</p></a></li>
          </div>
            <div name="siderow" class="row">
              <li><a href="#"><img src="assets/icons/ranking-cup.svg" class="image-responsive" style="width:30;float:left;"><p class="listname" >Ranks</p></a></li>
            </div>
        </ul>
     </div>
     <div id="page-content-wrapper" style="float:left;height:100%;">
       <div class="container-fluid" id="banner1">

           <div class="col-lg-12">
             <div  class="row" id="buttondiv" style="margin-bottom:6px;">
              <button id="menu-toggle2"  type="button" style="float:left;"><img src="assets/icons/slider.svg" class="image-responsive" style="width:30;"></button>
             </div>
                <div class="row">
                <div class="col-md-4" style="margin-left:2vw;margin-right:4vw;margin-top:4vw;">
                   <div class="card" style="">
                       <img class="card-img-top" src="assets/images/sakya/profile.jpg" alt="card image">
                       <div class="card-body">
                         <img style="width:10%;margin-right:3vw;float:left;" src="assets/university-solid.svg">
                         <h4 class="card-title" style="float:left;font-size:4vh;">Sakya Higher Educational Institute</h4>
                       </div>
                   </div>
                 </div>
                 <div class="col-md-6" style="margin-top:4vw;">
                     <% include partials/newsbox.ejs  %>
                 </div>
               </div>
              <div class="row" style="margin-top:14vh;">

                <div class="col-md-6" style="float:left">
                 <div class="card"  >

                 <div class="card  bg-primary" >

                   <p style="text-align:center;color:white;font-size:5vh;float:left;"><img src="assets/courses.svg" style="width:7vh;float:left;"></img>Available Courses</p>
                 </div>
                     <% for(var i=0; i< courses.length; i++){ %>
                     <a class="bg-light btn btn-light course-item" href="/courseView" style="padding:2vw;font-size:20;text-align:center;margin-bottom:1vh;"><img src="assets/course-black.svg" style="width:5vh;float:left;margin-rignt:2vw;"></img><%=courses[i].cname %></a>

                     <% } %>

               </div>
              </div>
              </div>
             </div>
         </div>
    </div>
    <div class="container-fluid" id="banner2" style="height:100vh;">
    <div class="row">


    </div>
    </div>
  </div>

</body>
</html>
